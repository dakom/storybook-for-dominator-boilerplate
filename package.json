{
  "name": "storybook-for-dominator-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo TODO: add tests",
    "start": "npm run clean && npm-run-all -p _media-server _storybook-watch",
    "storybook:build-release": "npm run clean && build-storybook -o ./dist/storybook-build && npm run _css-bundle && npm run _copy-media",
    "clean": "rimraf ./dist",
    "_css-bundle": "npm-run-all -p _css-bundle-dev _css-bundle-prod",
    "_css-bundle-dev": "postcss src/imports.css --env development -o dist/storybook-build/dist/styles.css",
    "_css-bundle-prod": "postcss src/imports.css --env production -o dist/storybook-build/dist/styles.min.css",
    "_media-server": "node ./build-utils/local-media-server.js",
    "_storybook-watch": "start-storybook -p 6006 -s ./dist",
    "_copy-media": "node ./build-utils/copy-media-directory.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "fs-extra": "^9.0.0",
    "html-to-dominator-string": "^1.15.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "restyle-loader": "0.0.3",
    "rimraf": "^3.0.2",
    "serve-index": "^1.9.1",
    "tailwindcss": "^1.4.6"
  }
}
