{
  "name": "storybook-for-dominator-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all -p _media-server _sass-watch _storybook-watch",
    "storybook:build-release": "npm run _sass-build-release && build-storybook -o ./dist/storybook-build && npm run _copy-static",
    "_copy-static": "node ./build-utils/copy-media-directory.js",
    "clean": "rimraf ./dist",
    "_media-server": "node ./build-utils/local-media-server.js",
    "_storybook-watch": "start-storybook -p 6006",
    "_sass-pre-build-release": "cross-env NODE_ENV=production node ./build-utils/write-dynamic-sass.js",
    "_sass-pre-build-dev": "cross-env NODE_ENV=development node ./build-utils/write-dynamic-sass.js",
    "_sass-watch": "npm run _sass-pre-build-dev && sass ./src/css/index.scss ./dist/index.css --watch",
    "_sass-build-release": "npm run _sass-pre-build-release && sass ./src/css/index.scss ./dist/index.css",
    "_sass-build-dev": "npm run _sass-pre-build-dev && sass ./src/css/index.scss ./dist/index.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "html-to-dominator-string": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "serve-index": "^1.9.1"
  }
}
